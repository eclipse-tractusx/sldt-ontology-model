#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdfs:label "This property refers to the hosted activities in a place."@en ,
                                 "hosts"@en .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#describesPhysicalObject
cx:describesPhysicalObject rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           rdfs:domain cx:ConceptualObject ;
                           rdfs:range cx:PhysicalObject ;
                           rdfs:comment "This property describes that a conceptual object has a descriptive relationship to a physical object."@en ;
                           rdfs:label "describes physical object"@en .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#hasParticipant
cx:hasParticipant rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain cx:Activity ;
                  rdfs:range cx:Actor ;
                  rdfs:comment "This property describes the active or passive participation of instances of Actors in an Activity." ;
                  rdfs:label "has participant"@en .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#hosts
cx:hosts rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#involvedIn
cx:involvedIn rdf:type owl:ObjectProperty ;
              rdfs:comment "This property describes in which activity the physical object is involved."@en ;
              rdfs:label "involved in"@en .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#participatesIn
cx:participatesIn rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain cx:Actor ;
                  rdfs:range cx:Activity ;
                  rdfs:comment "Inverse of 'has participant'"@en ;
                  rdfs:label "participates in"@en .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#refersToConceptualObject
cx:refersToConceptualObject rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf owl:topObjectProperty ;
                            rdfs:domain cx:Activity ;
                            rdfs:range cx:ConceptualObject ;
                            rdfs:comment "This property refers to actively or passively used conceptual objects in an Activity."@en ;
                            rdfs:label "refers to conceptual object"@en .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#refersToPhysicalObject
cx:refersToPhysicalObject rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf owl:topObjectProperty ;
                          rdfs:domain cx:Activity ;
                          rdfs:range cx:ConceptualObject ;
                          rdfs:comment "This property refers to actively or passively used physical objects in an Activity."@en ;
                          rdfs:label "refers to physical object"@en .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#relatedToPlace
cx:relatedToPlace rdf:type owl:ObjectProperty ;
                  rdfs:domain cx:Actor ;
                  rdfs:range cx:Place ;
                  rdfs:label "This property describes that an actor has a relationship to a particular place, such as reside, own, or manage, etc."@en ,
                             "related to place"@en .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#takesPlaceAt
cx:takesPlaceAt rdf:type owl:ObjectProperty ;
                rdfs:domain cx:Activity ;
                rdfs:range cx:Place ;
                rdfs:comment "This property describes the spatial location of an activity."@en ;
                rdfs:label "takes place at"@en .


