###  https://catena-x.net/product-knowledge/ontology/core/2023/05#endDateTime
cx:endDateTime rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain cx:Activity ;
               rdfs:range xsd:dateTime ;
               rdfs:comment "End time of the activity."@en ;
               rdfs:label "end datatime"@en .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#id
cx:id rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf owl:topDataProperty ;
      rdfs:domain owl:Thing ;
      rdfs:range xsd:string .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#name
cx:name rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdfs:range xsd:string .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#role
cx:role rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdfs:range xsd:string .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#startDateTime
cx:startDateTime rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain cx:Activity ;
                 rdfs:range xsd:dateTime ;
                 rdfs:comment "Start time of the activity."@en ;
                 rdfs:label "start datatime"@en .


#################################################################
#    Classes
#################################################################

###  https://catena-x.net/product-knowledge/ontology/core/2023/05#Activity
cx:Activity rdf:type owl:Class ;
            owl:disjointUnionOf ( cx:Actor
                                  cx:Address
                                  cx:ConceptualObject
                                  cx:PhysicalObject
                                  cx:Place
                                ) ;
            rdfs:comment "This class comprises actions intentionally carried out by instances of Actor that result in changes of state in physical and conceptual objects."@en .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#Actor
cx:Actor rdf:type owl:Class ;
         dc:description "This class comprises organization or people, either individually or in groups, who have the potential to perform intentional actions of kinds for which someone may be held responsible." .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#Address
cx:Address rdf:type owl:Class .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#ConceptualObject
cx:ConceptualObject rdf:type owl:Class ;
                    dc:description "This class includes non-material products, human-produced data related to physical objects. The production of such information may have been supported by the use of technical tools."@en ;
                    rdfs:label "Conceptual Object"@en .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#PhysicalObject
cx:PhysicalObject rdf:type owl:Class ;
                  dc:description "This class includes objects of a material nature, which are documentation units and have physical boundaries."@en ;
                  rdfs:label "Physical Object"@en .


###  https://catena-x.net/product-knowledge/ontology/core/2023/05#Place
cx:Place rdf:type owl:Class ;
         rdfs:comment "The class Place is determined by reference to the position of objects such as buildings, cities, or special geographic markers."@en .


